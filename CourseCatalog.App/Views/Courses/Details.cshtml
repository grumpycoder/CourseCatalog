
@{
    ViewBag.Title = "Course Detail";
    var isAdmin = "false";
    if (User.Identity.HasRoleClaim("CourseAdmin") || @User.Identity.HasRoleClaim("Admin"))
    {
        isAdmin = "true"; 
    };
}
<course-detail course-id="@Model" is-admin=@isAdmin></course-detail>


@section scripts{
    <script src="~/src/app/courses/course-detail.component.js"></script>

}
