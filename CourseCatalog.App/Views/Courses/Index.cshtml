@{
    ViewBag.Title = "Courses";
    var isAdmin = "false";
    if (User.Identity.HasClaim("CourseAdmin") || @User.Identity.HasClaim("Admin"))
    {
        isAdmin = "true"; 
    };
}

<course-list filter="@ViewBag.Filter" is-admin=@isAdmin></course-list>


@section scripts{
    <script src="~/src/app/courses/course-list.component.js"></script>
}
