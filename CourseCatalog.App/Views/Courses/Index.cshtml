@{
    ViewBag.Title = "Courses";
    var isAdmin = "false";
    if (User.Identity.HasRoleClaim("CourseAdmin") || User.Identity.HasRoleClaim("Admin"))
    {
        isAdmin = "true";
    }
}

<course-list filter="@ViewBag.Filter" is-admin="@isAdmin"></course-list>


@section scripts{
    <script src="~/src/app/courses/course-list.component.js"></script>
}