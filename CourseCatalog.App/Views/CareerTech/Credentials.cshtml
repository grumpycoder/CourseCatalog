
@{
    ViewBag.Title = "Credentials";
    var isAdmin = "false";
    if (User.Identity.HasClaim("CareerTechAdmin") || @User.Identity.HasClaim("Admin"))
    {
        isAdmin = "true";
    };
}

<credential-list is-admin=@isAdmin></credential-list>

@section scripts{
    <script src="~/src/app/careertech/credentials/credential-list.component.js"></script>

}