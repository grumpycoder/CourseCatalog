@model int
@{
    ViewBag.Title = "Program Detail";
    var isAdmin = "false";
    if (User.Identity.HasRoleClaim("CareerTechAdmin") || User.Identity.HasRoleClaim("Admin"))
    {
        isAdmin = "true";
    }
}

<program-detail program-id="@Model" is-admin="@isAdmin"></program-detail>


@section scripts{
    <script src="~/src/app/careertech/programs/program-detail.component.js"></script>
}