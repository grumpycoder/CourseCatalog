@model int
@{
    ViewBag.Title = "Credential Detail";
    var isAdmin = "false";
    if (User.Identity.HasRoleClaim("CareerTechAdmin") || User.Identity.HasRoleClaim("Admin"))
    {
        isAdmin = "true";
    }
}

<credential-detail credentialid="@Model" is-admin="@isAdmin"></credential-detail>


@section scripts{
    <script src="~/src/app/careertech/credentials/credential-detail.component.js"></script>
}