@{
    ViewBag.Title = "Cluster Detail";
    var isAdmin = "false";
    if (User.Identity.HasRoleClaim("CareerTechAdmin") || User.Identity.HasRoleClaim("Admin"))
    {
        isAdmin = "true";
    }
}

<cluster-detail cluster-id="@Model" is-admin="@isAdmin"></cluster-detail>


@section scripts{
    <script src="~/src/app/careertech/clusters/cluster-detail.component.js"></script>
}